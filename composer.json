{
  "name": "loveorigami/lo-module-gallery",
  "description": "Gallery module for Lo-CMS",
  "version": "1.0",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "gallery",
    "module"
  ],
  "homepage": "https://github.com/loveorigami/lo-module-gallery",
  "time": "2016-08-24",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrey Lukyanov",
      "email": "loveorigami@mail.ru",
      "homepage": "http://loveorigami.info",
      "role": "Developer"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2",
    "abeautifulsite/simpleimage": "~3.2",
    "devgroup/yii2-dropzone": "*",
    "bower-asset/lightgallery": "~1.4"
  },
  "autoload": {
    "psr-4": {
      "lo\\modules\\gallery\\": "src/"
    }
  },
  "extra": {
    "bootstrap": "lo\\modules\\gallery\\Bootstrap",
	"config-plugin": {
		"admin": [
			"src/modules/admin/config/main.php"
		],
		"cli": [
			"src/config/cli.php"
		]
	}
  }
}
